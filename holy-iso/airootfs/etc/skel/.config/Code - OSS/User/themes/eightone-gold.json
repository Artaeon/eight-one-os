    "tokenColors": [
        {
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#595959",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "string",
                "string.quoted"
            ],
            "settings": {
                "foreground": "#2ecc71"
            }
        },
        {
            "scope": [
                "constant.numeric",
                "constant.language"
            ],
            "settings": {
                "foreground": "#d4af37"
            }
        },
        {
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#9b59b6"
            }
        },
        {
            "scope": [
                "keyword.control",
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#e74c3c"
            }
        },
        {
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#3498db"
            }
        },
        {
            "scope": [
                "entity.name.type",
                "entity.name.class",
                "support.type"
            ],
            "settings": {
                "foreground": "#1abc9c"
            }
        },
        {
            "scope": [
                "variable",
                "variable.other"
            ],
            "settings": {
                "foreground": "#e8e6df"
            }
        },
        {
            "scope": [
                "variable.parameter"
            ],
            "settings": {
                "foreground": "#f0d78c",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "entity.name.tag"
            ],
            "settings": {
                "foreground": "#e74c3c"
            }
        },
        {
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#d4af37",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "support.constant",
                "constant.other"
            ],
            "settings": {
                "foreground": "#d4af37"
            }
        },
        {
            "scope": [
                "meta.decorator",
                "punctuation.decorator"
            ],
            "settings": {
                "foreground": "#d4af37",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "markup.heading"
            ],
            "settings": {
                "foreground": "#d4af37",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "markup.inline.raw"
            ],
            "settings": {
                "foreground": "#2ecc71"
            }
        }
    ]
}